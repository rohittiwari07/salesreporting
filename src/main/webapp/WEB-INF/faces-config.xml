<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
              version="2.2">
    <name>spreporting</name>
    <component>
        <component-type>com.rtiwari.sales.theme.menu.OmegaMenu</component-type>
        <component-class>com.rtiwari.sales.theme.menu.OmegaMenu</component-class>
    </component>
    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>com.rtiwari.sales.theme.menu.OmegaMenuRenderer</renderer-type>
            <renderer-class>com.rtiwari.sales.theme.menu.OmegaMenuRenderer</renderer-class>
        </renderer>
    </render-kit>
    <application>
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
    <lifecycle>
        <phase-listener>org.springframework.web.jsf.DelegatingPhaseListenerMulticaster</phase-listener>
    </lifecycle>
    <converter>
        <converter-id>Contacts</converter-id>
        <converter-class>com.rtiwari.sales.model.Contacts</converter-class>
    </converter>
</faces-config>